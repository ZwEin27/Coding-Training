*/
public class No_1_Two_Sum {

	public static void main(String[] args) {

	}
	
	public int[] twoSum(int[] numbers, int target) {
		int sum = 0;
		int[] result = {0,0};
		for(int i=0; i<numbers.length-1; i++){
			for(int j=1; j<numbers.length-1; j++){
				sum = numbers[i] + numbers[j];
				 if (sum == target) {
					 
				 }
			}
		}
		
		return numbers;
    }

}